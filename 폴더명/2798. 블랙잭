# https://www.acmicpc.net/problem/2798

from itertools import combinations  #조합 만들 함수 설치

# N, M 입력받기
N, M = map(int, input().split())  

# 숫자 배열을 입력받음
nums = list(map(int, input().split()))

# 
result = 0

# 조합으로 세 개의 숫자 선택
for aa in combinations(nums, 3):
    # 선택된 숫자들의 합이 M 이하인 경우에 최대 합 갱신
    if sum(cards) <= M:
        result = max(result, sum(cards))

# 최종 결과 출력
print(result)
