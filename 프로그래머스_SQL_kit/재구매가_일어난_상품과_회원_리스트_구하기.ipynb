      "source": [
        "# ONLINE_SALE 테이블에서 동일한 회원이 동일한 상품을 재구매한 데이터\n",
        "SELECT USER_ID, PRODUCT_ID\n",
        "FROM ONLINE_SALE\n",
        "GROUP BY USER_ID, PRODUCT_ID\n",
        "# having 은 group by 랑 같이 쓴다\n",
        "HAVING COUNT(*)>1\n",
        "# 오름차순 ASC 귀찮아도 작성하기\n",
        "ORDER BY USER_ID ASC, PRODUCT_ID DESC"
